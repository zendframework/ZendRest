    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "rest"
    ],
    "homepage": "http://packages.zendframework.com/",
    "autoload": {
        "psr-0": {
            "ZendRest": "library/"
